@using Simco.CrossInfraestructureLayer
@using Simco.DomainModelLayer.ViewModels

@model IndicadorModel

<h2>Indicadores de gestión</h2>

<div class="reporte">
    <div class="reporte-nombre">
        Curadurías
    </div>
    <div class="reporte-descripcion">
        Retorna el listado de todas las curadurías.
    </div>
    <div class="reporte-filtro">
        @{
            string url = string.Format("{0}?reporte={1}", @Url.Content("~/Reports/visorreporte.aspx"), ListaReportes.Curadurias);
            <a href="@url" class="boton" target="_blank">Cargar reporte</a>
        }
    </div>
</div>
<div class="reporte">
    <div class="reporte-nombre">
        Documentos
    </div>
    <div class="reporte-descripcion">
        Retorna el listado de todos los documentos que han sido cargados al Sistema.
    </div>
    <div class="reporte-filtro">
        @{
            url = string.Format("{0}?reporte={1}&urldocumentos={2}", @Url.Content("~/Reports/visorreporte.aspx"), ListaReportes.Documentos, @Url.Action("Index", "Documentos"));
            <a href="@url" class="boton" target="_blank">Cargar reporte</a>
        }
    </div>
</div>
<div class="reporte">
    <div class="reporte-nombre">
        Encuestas por usuario
    </div>
    <div class="reporte-descripcion">
        Retorna las respuestas seleccionadas por un usuario para una determinada encuesta.
    </div>
    <div class="reporte-filtro">
        <div class="formulario-label">
            Lista de usuarios
        </div>
        @Html.DropDownListFor(model => model.UsuarioId, new SelectList(Model.Usuarios, "UsuarioId", "Nombre", Model.UsuarioId), "Seleccione un usuario...", new { @class = "chosen", style = "width:559px;" })
        <div id="divencuestas" style="margin-top: 5px;">
            <div class="formulario-label">
                Lista de encuestas respondidas
            </div>
            @Html.DropDownListFor(model => model.EncuestaId, Enumerable.Empty<SelectListItem>(), "Seleccione una encuesta...", new { @class = "chosen", style = "width:556px;" })
        </div>
        <div id="divrespuestas" style="margin-top: 5px;">
            <div class="formulario-label">
                Lista de respuestas
            </div>
            @Html.DropDownListFor(model => model.RespuestaId, Enumerable.Empty<SelectListItem>(), "Seleccione una fecha...", new { @class = "chosen", style = "width:556px;" })
        </div>
        <div id="divboton" style="margin-top: 15px;">
            <a href="#" id="linkencuestas" class="boton" target="_blank" targeturl="@Url.Content("~/Reports/visorreporte.aspx")" targetreporte="@Html.Raw(ListaReportes.EncuestaResultadosPorUsuario.ToString())">Cargar reporte</a>
        </div>
    </div>
</div>
